package JSONParsers;

import java.io.IOException;
import java.util.List;

import DataBinders.SearchResult;

import com.fasterxml.jackson.core.JsonParseException;
import com.fasterxml.jackson.core.type.TypeReference;
import com.fasterxml.jackson.databind.JsonMappingException;
import com.fasterxml.jackson.databind.ObjectMapper;


public class ParseNearbySearch {
	
	public List<SearchResult> parse(String data) throws JsonParseException, JsonMappingException, IOException {
		ObjectMapper mapper = new ObjectMapper();
		List<SearchResult> result = mapper.readValue(data, new TypeReference<List<SearchResult>>() { });
		return result;
	}
}
