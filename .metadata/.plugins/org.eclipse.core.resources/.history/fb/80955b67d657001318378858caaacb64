package JSONParsers;

import java.io.IOException;
import java.util.List;

import DataBinders.NearbySearchResult;

import com.fasterxml.jackson.core.JsonParseException;
import com.fasterxml.jackson.core.type.TypeReference;
import com.fasterxml.jackson.databind.JsonMappingException;
import com.fasterxml.jackson.databind.ObjectMapper;


public class ParseNearbySearch {
	
	public static List<NearbySearchResult> parse(String data) throws JsonParseException, JsonMappingException, IOException {
		ObjectMapper mapper = new ObjectMapper();
		List<NearbySearchResult> result = mapper.readValue(data, new TypeReference<List<NearbySearchResult>>() { });
		return result;
	}
}
